	<queryString>
		<![CDATA[SELECT 
    city, 
    country, 
    zip_code, 
    state, 
    neighborhood,
    COUNT(*) AS total_addresses
FROM 
    public.address
GROUP BY 
    city, country, zip_code, state, neighborhood
ORDER BY 
    total_addresses DESC]]>
	</queryString>
